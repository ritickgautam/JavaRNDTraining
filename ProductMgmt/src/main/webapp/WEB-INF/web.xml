<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateUserServlet</servlet-name>
  	<display-name>CreateUserServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.CreateUserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NewProductServlet</servlet-name>
  	<display-name>NewProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.NewProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<display-name>DeleteProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.DeleteProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditProductServlet</servlet-name>
  	<display-name>EditProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.EditProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateProductServlet</servlet-name>
  	<display-name>UpdateProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.Controller.UpdateProductServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateUserServlet</servlet-name>
  	<url-pattern>/create</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NewProductServlet</servlet-name>
  	<url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteProductServlet</servlet-name>
  	<url-pattern>/deleteProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditProductServlet</servlet-name>
  	<url-pattern>/editProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateProductServlet</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
</web-app>
