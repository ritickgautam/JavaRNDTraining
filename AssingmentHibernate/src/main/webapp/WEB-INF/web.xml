<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>GetAllCountry</servlet-name>
		<display-name>GetAllCountry</display-name>
		<description></description>
		<servlet-class>com.Controller.GetAllCountry</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>getAllLangs</servlet-name>
		<display-name>getAllLangs</display-name>
		<description></description>
		<servlet-class>com.Controller.getAllLangs</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GetAllCountry</servlet-name>
		<url-pattern>/getCountries</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>getAllLangs</servlet-name>
		<url-pattern>/getAllLangs</url-pattern>
	</servlet-mapping>
</web-app>
